<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.system.entity.po.VUserRole" table="v_userrole">
       <id name="urid" column="urid" type="java.lang.Long">            
            <generator class="assigned"/>
        </id>
        <property name="useraccount" column="USERACCOUNT" type="java.lang.String"/>
        <property name="userid" column="USERID" type="java.lang.Long"/>
        <property name="roleid" column="ROLEID" type="java.lang.Long"/>
    </class>
</hibernate-mapping>
