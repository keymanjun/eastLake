<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.system.entity.po.SysRoleMenu" table="sys_role_menu" dynamic-update="false">
        <id name="id" column="ID" type="java.lang.Long">
            <generator class="native"/>
        </id>
        <property name="menucode" column="MENUCODE" type="java.lang.String"/>
        <property name="roleid" column="ROLEID" type="java.lang.Long"/>            
       
    </class>
</hibernate-mapping>
