<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts>
    <package name="employee" extends="struts-default">
       
        <action name="employeeInfoAction" class="com.application.employee.basic.action.EmployeeInfoAction">
			<result name="employeeAdd">/WEB-INF/pages/employee/employeeInfoAdd.jsp</result>
			<result name="employeeEdit">/WEB-INF/pages/employee/employeeInfoEdit.jsp</result>
			<result name="employeeInfo">/WEB-INF/pages/employee/employeeInfo.jsp</result>
			<result name="employeeList">/WEB-INF/pages/employee/employeeInfoList.jsp</result>
			<result name="employeeLoad">/WEB-INF/pages/employee/employeeInfoLoad.jsp</result>
			<result name="employeeToExport">/WEB-INF/pages/employee/employeeInfoToExport.jsp</result>
			<result name="employeeExport">/WEB-INF/pages/employee/employeeExport.xls</result>
    	</action>
    </package> 
			
</struts>
